    "version": "0.2.0",
    "configurations": [
        {
            "name": "swarm",
            "type": "python",
            "request": "launch",
            "program": "/usr/local/bin/swarm",
            "console": "integratedTerminal",
            "args": [
                "-f",
                "../pes/locust/mix.py",
                "-t",
                "360",
                "-u",
                "100",
                "-L",
                "DEBUG",
                // "-H",
                // "http://example.com",
                "--loadgens",
                "2"
            ],
            "env": {
                "LOCUST_RPS": "1",
                "LC_ALL": "en_US.UTF-8"
            },
            "justMyCode": false
        }
    ]
}